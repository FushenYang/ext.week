{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "week",
  "title": "获取时间戳",
  "description": "get the day string",
  "icon": "command-icon.png",
  "author": "yangfs",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "week",
      "title": "week",
      "description": "get a date string for log",
      "mode": "no-view"
    },
    {
      "name": "label",
      "title": "label",
      "description": "get a label for dir",
      "mode": "no-view"
    }
    ,
    {
      "name": "st",
      "title": "stamp",
      "description": "get a time stamp",
      "mode": "no-view"
    }
    ,
    {
      "name": "day",
      "title": "day",
      "description": "get today like xxxx-xx-xx",
      "mode": "no-view",

      "arguments": [
        {
          "name": "shift",
          "placeholder": "shift",
          "type": "text"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}